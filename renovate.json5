{
  extends: [
    "config:recommended",
    ":separateMultipleMajorReleases",
    ":ignoreUnstable",
    ":dependencyDashboard",
  ],
  configMigration: true,
  "packageRules": [
    {
      "matchSourceUrlPrefixes": ["github.com/acaylor"],
      "versioning": "regex:^((?<compatibility>.*)-v|v*)(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)$"
    }
  ]
}
